<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <title>Layout Example</title>
    </head>
    <referenceBlock name="page.main.title">
        <action method="setPageTitle">
            <argument name="page_title" xsi:type="string">Something else</argument>
        </action>
    </referenceBlock>
    
    <!-- <referenceBlock name="page.main.title" remove="true"></referenceBlock> -->
    <referenceContainer name="content">
        <block class="Magento\Framework\View\Element\Template" name="layout_examples" template="Video7_LayoutExamples::layoutexamplesContent1.phtml" />
    </referenceContainer>
</page>

<!--class Magento\Framework\View\Element\Template is used by block class 
and in class attribute namespace of block is provided
but since it is not created we can do this-->